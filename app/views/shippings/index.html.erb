<div class="col-lg-12">
  <p id="notice"><%= notice %></p>

  <h1>Listing Shippings</h1>

  <table class="table table-bordered">
    <thead>
    <tr>
      <th> ID</th>
      <th> Xmlns</th>
      <th> Shipment ID</th>
      <th> Shipment Status</th>
      <th> Tracking Pin</th>
      <th> Creation</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @shippings.each do |shipping| %>
        <tr>
          <td> <%= shipping.id %> </td>
          <td> <%= shipping.xmlns %> </td>
          <td> <%= shipping.shipment_id %> </td>
          <td> <%= shipping.shipment_status %> </td>
          <td> <%= shipping.tracking_pin %> </td>
          <td> <%= shipping.created_at %> </td>
          <td><%= link_to 'Show', shipping %></td>
          <td><%= link_to 'Edit', edit_shipping_path(shipping) %></td>
          <td><%= link_to 'Destroy', shipping, method: :delete, data: {confirm: 'Are you sure?'} %></td>
        </tr>
    <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Shipping', new_shipping_path %>
</div>
